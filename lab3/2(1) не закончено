# Минимальный элемент заданного одномерного массива увеличить в 2 раза
n = int(input('Введите количество элементов массива'))
list = []
i = 0
x = 0
for i in range(n):
    list.append(int(input()))
y = min(list)
for x in range(n):
    if min(list) == list[x]:
        list[x] *= 2
        break
    else:
        continue
print(list)
